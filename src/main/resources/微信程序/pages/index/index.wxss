page {
	background: #f0f0f0;
	font-family: "微软雅黑",Helvetica, Arial, "Hiragino Sans GB", "Source Han Sans CN", "PingFang SC", Roboto,  "Heiti SC", "Microsoft Yahei", sans-serif;
	background: #fff;
}

.swiper-tab {
	margin-top: 10px;
	height: 20px;
	padding: 10px 0;
	background: #fff;
	display: flex;
	font-size: 17px;
	line-height: 20px;
	border-bottom: 1px solid #e6e6e6;
}

.swiper-tab-list {
	flex: 1;
	text-align: center;
	position: relative;
	color: #787878;
}

.swiper-tab-list:first-of-type {
	border-right: 1px solid #c8c8c8;
}

.swiper-tab-list.on {
	color: #fa4b22;
}

.swiper-tab-list.on::after {
	content: "";
	height: 2px;
	width: 100px;
	background: #fa4b22;
	position: absolute;
	left: 50%;
	margin-left: -50px;
	bottom: -10px;
}

.book-box,.order-box {
	width: 100%;
	position: relative;
	box-sizing: border-box;
}

.swiper-vertical-box {
	width: 100%;
	padding-left: 100px;
	box-sizing: border-box;
}

.order-box {
	background: #fff;
}
/*.swiper-vertical-tab{width:100px;position:absolute;left:0px;top:0;min-height:100%;}*/
.swiper-vertical-tab {
	width: 100px;
	position: absolute;
	left: 0px;
	top: 0;
}

.swiper-vertical-tab view {
	height: 50px;
	font-size: 16px;
	color: #505050;
	text-align: center;
	box-sizing: border-box;
	padding-top: 20px;
}

.swiper-vertical-tab {
	background: #eee;
}

.swiper-vertical-tab view.on {
	background: #fff;
}

.swiper-vertical-box .vertical-list {
	height: 100%;
}

.swiper-vertical-box .vertical-list .title {
	height: 40px;
	padding-left: 20px;
	background: #f5f5f5;
	color: #646464;
	line-height: 40px;
	font-size: 16px;
	display: block;
}

.type-detail {
	background: #fff;
	padding-bottom: 10px;
}

.type-detail>view {
	border-top: 1px solid #E5E5E5;
	border-bottom: 1px solid #E5E5E5;
	margin-left: 10px;
	overflow: hidden;
	padding-left: 80px;
	box-sizing: border-box;
	position: relative;
	height: 92px;
	width: 100%;
	margin-bottom: -1px;
}

.type-detail view image {
	width: 70px;
	height: 70px;
	border-radius: 6px;
	margin-top: 10px;
	border: 1px solid #ccc;
	float: left;
	margin-bottom: 10px;
	position: absolute;
	left: 0;
	top: 0;
}

.book-detail {
	float: left;
	width: 100%;
}

.book-detail>text {
	font-size: 16px;
	color: #505050;
	margin-top: 16px;
	display: block;
}

.book-detail button,.book-result-detail button {
	float: left;
	width: 27px;
	height: 28px;
	border-radius: 14px;
}

.book-detail>view text {
	float: left;
}

.book-detail>view view {
	float: left;
}

.book-detail>view {
	margin-top: 5px;
	width: 100%;
	overflow: hidden;
	position: relative;
	height: 28px;
}

